+{
+	Ecore::BusTimeout* t = reinterpret_cast<Ecore::BusTimeout*>(data);
+
+	debug_log("Ecore::BusTimeout::timeout_handler( void *data )");
+
+	t->handle();
+
+	return 1; // 1 -> reshedule in ecore for next timer interval
